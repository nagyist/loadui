    <artifactId>loadui-soapui-plugin</artifactId>
    <packaging>bundle</packaging>
    <name>loadUI soapUI Plugin</name>
    <description>Allows loadUI to run soapUI tests.</description>

    <properties>
        <loadUIVersion>${project.version}</loadUIVersion>
        <soapUIVersion>5.1.0-SNAPSHOT</soapUIVersion>
    </properties>
    <repositories>
        <repository>
            <id>eviware</id>
            <name>Eviware Maven2 Repository</name>
            <url>http://www.eviware.com/repository/maven2</url>
        </repository>
        <repository>
            <id>central</id>
            <name>Central Maven2 Repository</name>
            <url>http://repo1.maven.org/maven2</url>
        </repository>
        <repository>
            <id>jboss</id>
            <name>JBoss Maven2 Repository</name>
            <url>http://repository.jboss.com/maven2/</url>
        </repository>
    </repositories>
    <distributionManagement>
        <repository>
            <name>Eviware Maven2 Repository</name>
            <id>eviware-maven2-ftp</id>
            <url>${eviware.maven2.repo}</url>
            <uniqueVersion>false</uniqueVersion>
        </repository>
    </distributionManagement>
    <build>
        <extensions>
            <!-- Enabling the use of FTP -->
            <extension>
                <groupId>org.apache.maven.wagon</groupId>
                <artifactId>wagon-ftp</artifactId>
                <version>2.3</version>
            </extension>
        </extensions>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-surefire-plugin</artifactId>
                <version>2.14.1</version>
                <configuration>
                    <excludedGroups>org.loadui.testfx.categories.TestFX</excludedGroups>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>2.3.1</version>
                <configuration>
                    <source>1.7</source>
                    <target>1.7</target>
                    <!-- Memory management -->
                    <fork>true</fork>
                    <meminitial>128m</meminitial>
                    <maxmem>512m</maxmem>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.apache.felix</groupId>
                <artifactId>maven-bundle-plugin</artifactId>
                <extensions>true</extensions>
                <version>2.3.4</version>
                <configuration>
                    <instructions>
                        <Bundle-Activator>com.eviware.loadui.components.soapui.SoapUIComponentActivator
                        </Bundle-Activator>
                        <Embed-Dependency>*;scope=provided</Embed-Dependency>
                        <Import-Package>
                            !com.eviware.loadui.api.charting.line,!com.eviware.loadui.api.reporting,!org.tbee.javafx.scene.layout,!com.apple.eawt,!sun.awt,!org.apache.commons.httpclient.*,!org.apache.tools.*,!org.fusesource.*,!org.apache.log,!org.apache.ivy.*,!org.apache.bsf.*,!org.apache.batik.*,!javax.xml.rpc.*,!jline,!org.apache.xml.resolver.*,!org.apache.axis.*,!org.apache.crimson.*;!org.mozilla.*,!com.sun.javadoc,!com.sun.tools.javadoc,!com.teamdev.xpcom.*,!com.teamdev.xulrunner-linux, !com.teamdev.xulrunner-mac,!com.teamdev.xulrunner-windows, !com.teamdev.xulrunner-linux64,!javax.servlet.jsp,!groovyjarjarasm.asm.util,!groovyjarjarasm.asm.tree.analysis,!org.dom4j.*,!org.apache.avalon.framework.logger,!org.apache.oro.text.regex,!com.sun.jdmk.comm,!com.sun.org.apache.xerces.internal.*,!com.sun.net.ssl.*,!com.sun.crypto.provider,!com.sun.java.swing.plaf.windows.*,!sun.io,!sun.misc,!sun.reflect.generics.reflectiveObjects,!org.jgroups*,!com.adbs*,!hermes*,!com.eviware.soapui.coverage*,!com.toedter.calendar*,!net.infonode.docking*,!net.infonode.properties.gui.util*,!net.infonode.tabbedpanel*,!net.infonode.util*,!*net.sf.jasperreports*,!org.jfree.util*,!com.jniwrapper*,!net.sf.cglib.proxy*,!org.codehaus.jettison*,!org.jdom*,!org.xmlpull.mxp1*,!org.xmlpull.v1*,!sun.reflect*,!net.sf.saxon.xpath,!org.apache.http*,!com.jp.protection*,!net.jcip.annotations*,!com.werken.xpath*,!org.apache.log.*,!org.apache.oro*,!org.owasp.*,*
                        </Import-Package>
                    </instructions>
                </configuration>
            </plugin>
            <plugin>
                <artifactId>maven-antrun-plugin</artifactId>
                <version>1.7</version>
                <executions>
                    <execution>
                        <phase>compile</phase>
                        <configuration>
                            <target xmlns:fx="javafx:com.sun.javafx.tools.ant">
                                <taskdef resource="com/sun/javafx/tools/ant/antlib.xml"
                                         uri="javafx:com.sun.javafx.tools.ant">
                                    <classpath>
                                        <pathelement location="${env.JAVA_HOME}/lib/ant-javafx.jar"/>
                                        <pathelement location="${env.JAVA_HOME}/jre/lib/ext/jfxrt.jar"/>
                                    </classpath>
                                </taskdef>

                                <fx:csstobin outdir="${basedir}/target/classes">
                                    <fileset dir="src/main/java" includes="**/*.css"/>
                                </fx:csstobin>

                                <fx:csstobin outdir="${basedir}/target/classes">
                                    <fileset dir="src/main/resources" includes="**/*.css"/>
                                </fx:csstobin>
                            </target>
                        </configuration>
                        <goals>
                            <goal>run</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>
        </plugins>
        <pluginManagement>
            <plugins>
                <!--This plugin's configuration is used to store Eclipse m2e settings
                    only. It has no influence on the Maven build itself. -->
                <plugin>
                    <groupId>org.eclipse.m2e</groupId>
                    <artifactId>lifecycle-mapping</artifactId>
                    <version>1.0.0</version>
                    <configuration>
                        <lifecycleMappingMetadata>
                            <pluginExecutions>
                                <pluginExecution>
                                    <pluginExecutionFilter>
                                        <groupId>org.apache.felix</groupId>
                                        <artifactId>maven-bundle-plugin</artifactId>
                                        <versionRange>[2.3.0,)</versionRange>
                                        <goals>
                                            <goal>run</goal>
                                            <goal>package</goal>
                                        </goals>
                                    </pluginExecutionFilter>
                                    <action>
                                        <ignore/>
                                    </action>
                                </pluginExecution>
                                <pluginExecution>
                                    <pluginExecutionFilter>
                                        <groupId>
                                            org.apache.maven.plugins
                                        </groupId>
                                        <artifactId>
                                            maven-antrun-plugin
                                        </artifactId>
                                        <versionRange>
                                            [1.7,)
                                        </versionRange>
                                        <goals>
                                            <goal>run</goal>
                                        </goals>
                                    </pluginExecutionFilter>
                                    <action>
                                        <ignore></ignore>
                                    </action>
                                </pluginExecution>
                            </pluginExecutions>
                        </lifecycleMappingMetadata>
                    </configuration>
                </plugin>
            </plugins>
        </pluginManagement>
    </build>
    <profiles>
        <profile>
            <id>gui-tests</id>
            <build>
                <plugins>
                    <plugin>
                        <artifactId>maven-failsafe-plugin</artifactId>
                        <version>2.14.1</version>
                        <configuration>
                            <forkMode>always</forkMode>
                            <groups>org.loadui.testfx.categories.TestFX</groups>
                            <failIfNoTests>true</failIfNoTests>
                            <testFailureIgnore>false</testFailureIgnore>
                        </configuration>
                        <dependencies>
                            <dependency>
                                <groupId>org.apache.maven.surefire</groupId>
                                <artifactId>surefire-junit47</artifactId>
                                <version>2.14</version>
                            </dependency>
                        </dependencies>
                        <executions>
                            <execution>
                                <goals>
                                    <goal>integration-test</goal>
                                </goals>
                                <configuration>
                                    <includes>
                                        <include>**/*.class</include>
                                    </includes>
                                </configuration>
                            </execution>
                        </executions>
                    </plugin>
                </plugins>
            </build>
        </profile>
        <profile>
            <id>install</id>
            <build>
                <plugins>
                    <plugin>
                        <artifactId>maven-antrun-plugin</artifactId>
                        <executions>
                            <execution>
                                <id>install-publish files</id>
                                <phase>install</phase>
                                <goals>
                                    <goal>run</goal>
                                </goals>
                                <configuration>
                                    <tasks>
                                        <copy todir="${publish.dir}">
                                            <fileset dir="${basedir}/target">
                                                <filename name="*.jar"/>
                                            </fileset>
                                        </copy>
                                    </tasks>
                                </configuration>
                            </execution>
                        </executions>
                    </plugin>
                </plugins>
            </build>
        </profile>
    </profiles>
